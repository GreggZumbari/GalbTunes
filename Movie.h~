/*
 * The header file for the Movie object. 
 * Holds the details of a single movie, including title, director, year, duration, and rating.
 * See Media.h for more details.
 */
#ifndef MOVIE_H
#define MOVIE_H

#include <vector>

class Movie {

 public:
  Movie();
  void setFields(char* title, char* artist, char* year, char* duration, char* pub);
  int getType();

 private:
  char* title;              
  char* artist;
  char* year;
  char* duration;
  char* pub;

#endif
